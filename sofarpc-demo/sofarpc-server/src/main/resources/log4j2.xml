<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="INFO" monitorInterval="30">

    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="[%-5p]:%d{yyyy-MM-dd HH:mm:ss} [%t] %c{1}:%L - %msg%n" />
        </Console>
    </Appenders>

    <Loggers>
        <root level="INFO">
            <appender-ref ref = "Console"/>
        </root>
    </Loggers>
</Configuration>